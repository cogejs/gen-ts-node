{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "noImplicitAny": false,
    "declaration": false,
    "sourceMap": false,
    "module": "CommonJS",
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "generators",
    "preserveConstEnums": true,
    "removeComments": true,
    "lib": [
      "ESNext"
    ],
    "target": "ES2015"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "src/__tests__"
  ]
}
